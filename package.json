{
  "name": "mnews",
  "version": "0.1.0",
  "description": "Nemo - Personal newsletter aggregation and digest system",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./dist/*",
    "./scripts/*": "./dist/scripts/*.js",
    "./lib/*": "./dist/lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/lib/*",
        "dist/scripts/*"
      ]
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:scripts": "tsc -p scripts/tsconfig.json",
    "build:ui": "tsx scripts/build-ui.ts",
    "update-imports": "tsx scripts/update-import-paths.ts",
    "fix-imports": "tsx scripts/fix-import-extensions.ts",
    "prebuild": "npm run build:lib && npm run build:ui",
    "update-import-extensions": "tsx scripts/update-import-extensions.ts",
    "type-check": "tsc --noEmit --moduleResolution bundler",
    "start": "next start",
    "postinstall": "tsc --build --clean && tsc --build",
    "lint": "next lint",
    "export": "npm run build && next export",
    "test": "echo 'Tests are currently disabled. To re-enable, rename __tests_old__ back to __tests__ and restore test scripts in package.json' && exit 0",
    "test:watch": "echo 'Tests are currently disabled. To re-enable, rename __tests_old__ back to __tests__ and restore test scripts in package.json' && exit 0",
    "test:coverage": "echo 'Tests are currently disabled. To re-enable, rename __tests_old__ back to __tests__ and restore test scripts in package.json' && exit 0",
    "prepare": "husky install",
    "ci:test": "npm test",
    "ci:build": "npm run build",
    "migrate:newsletters": "tsx scripts/migrate-newsletters.ts",
    "migrate:newsletters:dry-run": "tsx scripts/migrate-newsletters.ts --dry-run",
    "migrate:newsletters:run": "tsx scripts/migrate-newsletters.ts --no-dry-run",
    "migrate:test": "tsx scripts/test-migration.ts",
    "migrate:rollback": "tsx scripts/rollback-migration.ts --dry-run",
    "migrate:rollback:run": "tsx scripts/rollback-migration.ts --no-dry-run"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.test.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@/components/(.*)$": "<rootDir>/components/$1",
      "^@/lib/(.*)$": "<rootDir>/lib/$1",
      "^@/types/(.*)$": "<rootDir>/types/$1",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/.history/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(node-fetch|@testing-library)/)"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/dompurify": "^3.0.5",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.35.3",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "eslint-config-next": "14.0.4",
    "framer-motion": "^10.16.4",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.526.0",
    "next": "14.2.32",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.0",
    "redis": "^4.7.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/next": "^8.0.7",
    "@types/node": "^20.19.13",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "commander": "^14.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.5",
    "node-fetch": "^2.7.0",
    "node-mocks-http": "^1.17.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.39.1",
    "zod": "^4.1.5"
  },
  "keywords": [
    "newsletter",
    "aggregation",
    "email",
    "digest",
    "news"
  ],
  "author": "Maurice Gaston",
  "license": "MIT"
}
